{
  "name": "app",
  "description": "Social Media Dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.1.3",
    "@material-ui/icons": "4.2.1",
    "apollo-cache-inmemory": "1.6.2",
    "apollo-client": "2.6.3",
    "apollo-link-http": "1.5.15",
    "graphql-tag": "2.10.1",
    "history": "4.9.0",
    "react": "^16.8.6",
    "react-apollo": "2.5.8",
    "react-dom": "^16.8.6",
    "react-router-dom": "5.0.1",
    "react-router-ext": "0.0.1",
    "react-scripts": "3.0.1"
  },
  "devDependencies": {
    "@testing-library/react": "8.0.2",
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-plugin-jest": "22.5.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.13.0",
    "jest-dom": "3.4.0",
    "lint-staged": "8.1.7",
    "prettier": "1.17.1",
    "sharp": "^0.22.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:staged": "cross-env CI=true react-scripts test --env=jsdom --verbose --coverage --findRelatedTests",
    "eject": "react-scripts eject",
    "format": "eslint -c .eslintrc ./src/**/*.js --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!**/__test__/utils/**",
      "!**/{__stories__,__mocks__}/**"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
